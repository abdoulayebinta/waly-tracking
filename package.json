{
  "name": "waly-tracking",
  "version": "1.0.0",
  "description": "Track my Job Search",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "deploy": "cd client && npm install && npm run build",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client \"",
    "start": "node server.js"
  },
  "keywords": [
    "MongoDB",
    "Express.js",
    "Node.js",
    "React.js",
    "MERN",
    "JWT"
  ],
  "author": "Abdoulaye Binta Bah",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  }
}
